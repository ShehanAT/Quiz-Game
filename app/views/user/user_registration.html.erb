<%= form_with( method: "get") do %>

<%= label_tag(:q, "Username:") %>
<%= text_field_tag(:username) %>

<%= label_tag(:q, "Email:") %>
<%= text_field_tag(:email) %>

<%= label_tag(:q, "Password:") %>
<%= text_field_tag(:password) %>

<%= label_tag(:q, "Confirm Password:") %>
<%= text_field_tag(:confirm_password) %>

<%= submit_tag %>

<% end %>