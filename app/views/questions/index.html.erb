<table>
    <thead>
   
        <td>Question Name</td>
        <td>Corresponding Quiz</td> 
        <td>Quiz Category</td>
    </thead>
    <tbody>
    <% @questions.each_with_index do |question, index| %>
        <tr>
            <td><%= link_to "#{question.question}", question_path(question.id), class:'question_name' %></td>
            <td class='quiz_name'><a href="/quizzes/<%= @results[index]["id"] %>"><%= @results[index]["name"] %></a></td>
            <td class='quiz_category'><%= @results[index]["category"] %></td>
        </tr>
    <% end %> 
    </tbody>
</table>
<%= button_to "Back To Home", root_url, id:"homepage_link", method: :get %>
